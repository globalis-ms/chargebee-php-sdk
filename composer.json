{
  "name": "globalis/chargebee-php-sdk",
  "description": "PHP Client for using the Chargebee API.",
  "license": "MIT",
  "keywords": ["chargebee", "api-client"],
  "type": "library",
  "authors": [
    {
      "name": "Pierre Dargham",
      "homepage": "https://github.com/pierre-dargham/"
    },
    {
      "name": "GLOBALIS media systems",
      "homepage": "https://www.globalis-ms.com/"
    },
    {
      "name": "Nathan Dunn",
      "homepage": "https://nathandunn.uk"
    },
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "psr/http-message": "^1.0",
    "php-http/message-factory": "^1.0",
    "php-http/discovery": "^1.7",
    "php-http/client-common": "^2.1",
    "php-http/multipart-stream-builder": "^1.0",
    "php-http/message": "^1.7",
    "psr/http-client": "^1.0",
    "php-http/client-implementation": "^1.0",
    "http-interop/http-factory-guzzle": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "php-http/mock-client": "^1.3",
    "php-coveralls/php-coveralls": "^2.2",
    "phpunit/phpcov": "^7.0",
    "php-http/guzzle6-adapter": "^2.0",
    "guzzlehttp/psr7": "^1.6",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "Globalis\\Chargebee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
